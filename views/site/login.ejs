<%- include('header'); -%>
    <!-- Token Sale Proceeds -->
    <section id="contact-us" class="contact-us overview-block-pt pt400">
        <div class="container">
            <div class="row">
                <div class="col-lg-3"></div>
                <div class="col-lg-6">
                    <form id="contact" method="post">
                        <div class="heading-title  py-4">

                            <h2 class="title iq-tw-6">Login</h2>
                        </div>

                        <div class="contact-form">
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="section-field">
                                        <input class="require" id="contact_name" type="text" placeholder="sponsor id"
                                            name="name">
                                    </div>
                                    <!-- <div class="section-field">
                                        <input class="require" id="contact_email" type="email" placeholder="Email*"
                                            name="email">
                                    </div> -->
                                    <div class="section-field">
                                        <input class="require" id="contact_phone" type="text" placeholder="password*"
                                            name="phone">
                                    </div>


                                    <div class="d-block text-center">
                                        <button id="submit" name="submit" type="submit" value="Send"
                                            class="button iq-mt-15 pr-3 pl-3">login</button>
                                    </div>
                                </div>

                            </div>
                    </form>
                </div>
                <div class="col-lg-3"></div>

            </div>
            </div>
        </div>
        <%- include('footer'); -%>